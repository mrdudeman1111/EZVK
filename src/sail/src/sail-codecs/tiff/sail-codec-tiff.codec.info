# TIFF codec information
#
[codec]
layout=8
version=1.0.1
priority=HIGHEST
name=TIFF
description=Tagged Image File Format
magic-numbers=49 49 2A 00;4D 4D 00 2A
extensions=tif;tiff
mime-types=image/tiff;image/tiff-fx

[load-features]
features=STATIC;MULTI-PAGED;META-DATA;ICCP
tuning=

[save-features]
features=STATIC;MULTI-PAGED;META-DATA;ICCP
pixel-formats=BPP32-RGBA
compressions=ADOBE-DEFLATE;DEFLATE;JPEG;LZMA;LZW;NONE;PACKBITS;PIXAR-LOG;ZSTD
default-compression=JPEG
tuning=
